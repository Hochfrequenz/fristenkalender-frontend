{
  "name": "fristenkalender-frontend",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=20.19"
  },
  "scripts": {
    "start": "vite dev --open",
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier '**/*.{svelte,js,jsx,ts,tsx,html,css,scss,json,yml,md}' --write",
    "format:check": "prettier '**/*.{svelte,js,jsx,ts,tsx,html,css,scss,json,yml,md}' --check",
    "lint": "eslint 'src/**/*.{js,ts,svelte}' --fix",
    "lint:check": "eslint 'src/**/*.{js,ts,svelte}'",
    "test:e2e": "npx tsx e2e/run-tests.ts"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.1",
    "@sveltejs/adapter-auto": "^7.0.0",
    "@sveltejs/adapter-static": "^3.0.10",
    "@sveltejs/kit": "^2.50.0",
    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "@types/jquery": "^3.5.33",
    "@types/node": "^25.0.10",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.53.0",
    "autoprefixer": "^10.4.24",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-svelte": "^3.14.0",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "prettier-plugin-svelte": "^3.4.1",
    "sass": "^1.97.3",
    "scss": "^0.2.4",
    "svelte": "^5.49.1",
    "svelte-check": "^4.3.5",
    "tailwindcss": "^3.4.19",
    "testcontainers": "^11.11.0",
    "tslib": "^2.8.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  },
  "type": "module",
  "dependencies": {
    "@auth0/auth0-spa-js": "^2.13.1"
  }
}
